{
  "name": "zon-format",
  "displayName": "ZON Format",
  "description": "Syntax highlighting for ZON (Zero Overhead Notation) files",
  "version": "1.1.0",
  "publisher": "zon-format",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "zonf",
        "aliases": ["ZONF", "zonf"],
        "extensions": [".zonf"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "zonf",
        "scopeName": "source.zonf",
        "path": "./syntaxes/zonf.tmLanguage.json"
      }
    ]
  }
}
